#!/bin/sh

# Check decoding of utimensat syscall.

. "${srcdir=.}/init.sh"

run_prog > /dev/null
OUT="$LOG.out"
run_strace -e utimensat $args > "$OUT"

check_prog grep
LC_ALL=C grep -x "utimensat(.*" "$LOG" > /dev/null || {
        rm -f "$OUT"
        skip_ 'test executable does not use utimensat syscall'
}

match_diff "$OUT" "$LOG"

rm -f "$OUT"

exit 0
